{
  "name": "blog-frontend-ssr",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development TS_NODE_PROJECT='./server/tsconfig.json' ts-node-dev --respawn  --transpileOnly --ignore-watch=.next --ignore-watch=node_modules --ignore-watch=src server/index.ts",
    "build:next": "next build",
    "build:server": "tsc -p ./server",
    "start:server": "cross-env NODE_ENV=production node ./dist",
    "server": "npm-run-all -p build:server build:next -s start:server",
    "start": "next start -p 8088",
    "local": "next dev",
    "lint": "eslint src --ext .ts --ext .tsx --fix",
    "docker:package": "docker build -f NodeModulesDockerfile -t docker.carrotwu.com/blog/blog-ssr-node-modules . --no-cache",
    "docker:push": "docker push docker.carrotwu.com/blog/blog-ssr-node-modules",
    "docker:build": "yarn docker:package && yarn docker:push"
  },
  "dependencies": {
    "@carrotwu/generatoc": "^1.0.1",
    "@carrotwu/valine": "^1.0.4",
    "@webdeb/next-styles": "^1.1.0",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "cross-env": "^7.0.2",
    "date-fns": "^2.14.0",
    "github-markdown-css": "^4.0.0",
    "koa": "^2.12.0",
    "koa-router": "^9.0.1",
    "koa2-proxy-middleware": "^0.0.4",
    "marked": "^1.1.0",
    "module-alias": "^2.2.2",
    "next": "9.4.4",
    "next-images": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "prismjs": "^1.21.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.10",
    "@types/koa": "^2.11.3",
    "@types/koa-router": "^7.4.1",
    "@types/marked": "^0.7.4",
    "@types/node": "^14.0.9",
    "@types/prismjs": "^1.16.1",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/redux-thunk": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "babel-plugin-prismjs": "^2.0.1",
    "eslint": "^7.1.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react-hooks": "^4.0.4",
    "ts-node-dev": "^1.0.0-pre.49",
    "typescript": "^3.9.3",
    "prettier": "^2.1.2"
  },
  "_moduleAliases": {
    "@src": "./src",
    "@server": "./server",
    "@": "./"
  }
}
